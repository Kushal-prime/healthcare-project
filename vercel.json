{
  "version": 2,
  "name": "empowering-health",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://empowering-health-api.vercel.app"
  },
  "functions": {
    "server/index.js": {
      "maxDuration": 30
    }
  }
}
